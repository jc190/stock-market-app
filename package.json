{
  "name": "stock-market-app",
  "version": "0.0.1",
  "description": "Chart stocks in real time.",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jc190/stock-market-app"
  },
  "homepage": "https://github.com/jc190/stock-market-app",
  "bugs": "https://github.com/jc190/stock-market-app/issues",
  "keywords": [
    "MongoDB",
    "Mongoose",
    "full stack",
    "Express",
    "Node",
    "javascript",
    "Socket.io"
  ],
  "scripts": {
    "start": "concurrently --kill-others \"npm run dev\" \"npm run watch:scss\"",
    "dev": "NODE_ENV='development' nodemon server.js",
    "prod": "node server.js",
    "watch:scss": "npm run build:scss && node-sass -wr styles/main.scss public/css/main.css",
    "build:scss": "node-sass styles/main.scss public/css/main.css"
  },
  "author": "James Calhoun",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^3.5.1",
    "dotenv": "^1.2.0",
    "express": "^4.12.4",
    "express-session": "^1.11.3",
    "mongoose": "^4.1.0",
    "passport": "^0.3.0",
    "passport-github": "^1.0.0",
    "pug": "^2.0.0-rc.4"
  },
  "devDependencies": {
    "browser-sync": "^2.23.6",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11"
  }
}
